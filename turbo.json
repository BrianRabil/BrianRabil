{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"check-types": {
			"dependsOn": ["^check-types"],
			"inputs": ["**/*.{ts,tsx}", "tsconfig.json"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"lint": {},
		"lint:fix": {
			"cache": false
		}
	}
}
